<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Activities.DevelopersActivity">

    <ImageView
        android:id="@+id/imagebackdevelopers"
        android:layout_width="@dimen/_20sdp"
        android:layout_height="@dimen/_20sdp"
        android:layout_marginStart="@dimen/_6sdp"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginEnd="@dimen/_12sdp"
        android:src="@drawable/ic_leftarrow"
        android:tint="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


<!--kumar sir flipview-->
    <com.wajahatkarim3.easyflipview.EasyFlipView
        android:id="@+id/kumarsir"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:flipOnTouch="true"
        app:flipEnabled="true"
        app:flipDuration="400"
        app:flipType="vertical"
        app:flipFrom="front"
        app:autoFlipBack="true"
        app:autoFlipBackTime="1000">

        <!-- Back Layout Goes Here -->
        <include layout="@layout/kumarsir_layout_back"/>

        <!-- Front Layout Goes Here -->
        <include layout="@layout/kumarsir_layout_front"/>

    </com.wajahatkarim3.easyflipview.EasyFlipView>

    <com.wajahatkarim3.easyflipview.EasyFlipView
        android:id="@+id/santhoshanna"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:flipOnTouch="true"
        app:flipEnabled="true"
        app:flipDuration="400"
        app:flipType="vertical"
        app:flipFrom="front"
        app:autoFlipBack="true"
        app:autoFlipBackTime="1000"
        >

        <!-- Back Layout Goes Here -->
        <include layout="@layout/santhoshanna_layout_back"/>

        <!-- Front Layout Goes Here -->
        <include layout="@layout/santhoshanna_layout_front"/>

    </com.wajahatkarim3.easyflipview.EasyFlipView>

    <com.wajahatkarim3.easyflipview.EasyFlipView
        android:id="@+id/karthikanna"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:flipOnTouch="true"
        app:flipEnabled="true"
        app:flipDuration="400"
        app:flipType="vertical"
        app:flipFrom="front"
        app:autoFlipBack="true"
        app:autoFlipBackTime="1000"
        >

        <!-- Back Layout Goes Here -->
        <include layout="@layout/karthickanna_layout_back"/>

        <!-- Front Layout Goes Here -->
        <include layout="@layout/karthickanna_layout_front"/>

    </com.wajahatkarim3.easyflipview.EasyFlipView>

    <com.wajahatkarim3.easyflipview.EasyFlipView
        android:id="@+id/harikrishnan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:flipOnTouch="true"
        app:flipEnabled="true"
        app:flipDuration="400"
        app:flipType="vertical"
        app:flipFrom="front"
        app:autoFlipBack="true"
        app:autoFlipBackTime="1000"
        >

        <!-- Back Layout Goes Here -->
        <include layout="@layout/harikrishnan_layout_back"/>

        <!-- Front Layout Goes Here -->
        <include layout="@layout/harikrishnan_layout_front"/>

    </com.wajahatkarim3.easyflipview.EasyFlipView>


</LinearLayout>